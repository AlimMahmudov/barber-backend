 
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Service {
  id Int @id @unique @default(autoincrement())
  title  String
  type   String
  time   String
  price   String
}


model Video {
  id Int @id @unique @default(autoincrement())
  youtube String
}


model Master {
  id Int @id @unique @default(autoincrement())
  human  String
  photo   String
  work   String
}